{
  "kind": "collectionType",
  "collectionName": "shops",
  "info": {
    "name": "Shop",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "rating": {
      "type": "integer"
    },
    "type": {
      "type": "string",
      "required": true
    },
    "currentStatus": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "addressLine1": {
      "type": "string"
    },
    "addressLine2": {
      "type": "string"
    },
    "pincode": {
      "type": "string"
    },
    "area": {
      "model": "area"
    },
    "shopkeepers": {
      "via": "shop",
      "plugin": "users-permissions",
      "collection": "user"
    },
    "gstNumber": {
      "type": "string"
    },
    "openingYear": {
      "type": "date"
    },
    "gstRegistrationType": {
      "type": "string"
    },
    "websiteURL": {
      "type": "string"
    },
    "latitude": {
      "type": "decimal"
    },
    "longitude": {
      "type": "decimal"
    }
  }
}
