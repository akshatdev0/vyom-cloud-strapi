{
  "kind": "collectionType",
  "collectionName": "shops",
  "info": {
    "name": "Shop",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "rating": {
      "type": "integer"
    },
    "gstNumber": {
      "type": "string"
    },
    "openingYear": {
      "type": "date"
    },
    "gstRegistrationType": {
      "type": "string"
    },
    "websiteURL": {
      "type": "string"
    },
    "shopkeepers": {
      "collection": "shopkeeper",
      "via": "shops"
    },
    "orders": {
      "collection": "order",
      "via": "shop"
    },
    "holidays": {
      "collection": "calendar-event"
    },
    "businessTypes": {
      "collection": "business-type"
    },
    "billingAddress": {
      "model": "address"
    },
    "shippingAddresses": {
      "collection": "address"
    },
    "isActive": {
      "type": "boolean"
    },
    "isDeleted": {
      "type": "boolean",
      "default": false
    },
    "companyTradeConnections": {
      "via": "shop",
      "collection": "company-shop-trade-connection"
    }
  }
}
