{
  "kind": "collectionType",
  "collectionName": "order_lines",
  "info": {
    "name": "Order Line",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "index": {
      "type": "integer"
    },
    "order": {
      "via": "orderLines",
      "model": "order"
    },
    "productVariant": {
      "model": "product-variant"
    },
    "quantity": {
      "type": "integer",
      "default": 1,
      "required": true,
      "min": 1
    },
    "price": {
      "type": "decimal"
    },
    "appliedPriceRules": {
      "collection": "price-rule"
    },
    "productTitle": {
      "type": "string",
      "required": true
    },
    "productVariantTitle": {
      "type": "string"
    },
    "productVariantAttributes": {
      "type": "json"
    }
  }
}
