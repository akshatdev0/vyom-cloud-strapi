{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "name": "Company",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "pid": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "currentStatus": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "suspendStatus": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "businessType": {
      "model": "business-type"
    },
    "companyType": {
      "model": "company-type"
    },
    "addressLine1": {
      "type": "string"
    },
    "addressLine2": {
      "type": "string"
    },
    "pincode": {
      "type": "string"
    },
    "area": {
      "model": "area"
    },
    "sku": {
      "type": "string",
      "regex": "^[A-Z]{4}$",
      "required": true,
      "unique": true,
      "maxLength": 4,
      "minLength": 4
