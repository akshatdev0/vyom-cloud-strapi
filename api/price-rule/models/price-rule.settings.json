{
  "kind": "collectionType",
  "collectionName": "price_rules",
  "info": {
    "name": "Price Rule"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "minSellingQuantity": {
      "type": "integer",
      "default": 0
    },
    "startDateTime": {
      "type": "datetime"
    },
    "endDateTime": {
      "type": "datetime"
    },
    "computationType": {
      "type": "enumeration",
      "enum": [
        "FIXED_PRICE",
        "PERCENTAGE"
      ],
      "default": "FIXED_PRICE"
    },
    "computeValue": {
      "type": "json",
      "required": true
    }
  }
}
